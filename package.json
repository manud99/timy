{
  "name": "timy",
  "description": "A simple app to manage time entries during your work day.",
  "main": "dist/server.js",
  "author": "Manuel Dublanc",
  "version": "1.0.0",
  "license": "ISC",
  "scripts": {
    "build": "tsc && mix",
    "watch": "mix watch",
    "prod": "tsc && mix --production",
    "run-ts": "tsc -w",
    "run-node": "nodemon --watch dist dist/server/server.js",
    "start": "concurrently --raw npm:run-*"
  },
  "dependencies": {
    "@types/express": "^4.17.9",
    "@types/node": "^14.14.9",
    "axios": "^0.21.0",
    "concurrently": "^5.3.0",
    "express": "^4.17.1",
    "nodemon": "^2.0.6",
    "tailwindcss": "^2.0.1",
    "typescript": "^4.1.2",
    "vue": "^3.0.2"
  },
  "devDependencies": {
    "@vue/compiler-sfc": "^3.0.2",
    "cross-env": "^7.0.2",
    "laravel-mix": "^6.0.0-beta.14",
    "mix-tailwindcss": "^1.2.0",
    "postcss": "^8.1.9",
    "ts-loader": "^8.0.11",
    "vue-loader": "^16.0.0-rc.2"
  }
}
