{
    "name": "timy",
    "private": true,
    "version": "0.0.0",
    "main": "dist-server/main.js",
    "type": "module",
    "scripts": {
        "dev": "concurrently --raw npm:dev-*",
        "dev-ts": "tsc --build tsconfig.server.json -w --preserveWatchOutput",
        "dev-server": "cross-env PORT=8080 nodemon --watch dist-server dist-server/main.js",
        "build": "vue-tsc && vite build && tsc --build tsconfig.server.json",
        "preview": "vite preview",
        "check": "vue-tsc && prettier .",
        "prettier": "prettier --write ."
    },
    "prisma": {
        "seed": "node prisma/seed.js"
    },
    "dependencies": {
        "@prisma/client": "^4.10.1",
        "axios": "^1.3.2",
        "concurrently": "^7.6.0",
        "cross-env": "^7.0.3",
        "express": "^4.18.2",
        "nodemon": "^2.0.20",
        "vite-express": "^0.3.1",
        "vue": "^3.2.45"
    },
    "devDependencies": {
        "@babel/types": "^7.20.7",
        "@tailwindcss/forms": "^0.5.3",
        "@types/express": "^4.17.16",
        "@vitejs/plugin-vue": "^4.0.0",
        "autoprefixer": "^10.4.13",
        "postcss": "^8.4.21",
        "prettier": "^2.8.3",
        "prisma": "^4.9.0",
        "tailwindcss": "^3.2.4",
        "typescript": "^4.9.3",
        "vite": "^4.1.0",
        "vue-tsc": "^1.0.24"
    }
}
